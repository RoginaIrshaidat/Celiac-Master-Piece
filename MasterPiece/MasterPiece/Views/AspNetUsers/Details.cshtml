@model MasterPiece.Models.AspNetUser

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<body>
    <div class="main-panel">
        <div class="content-wrapper">
            <div class="row">
                <div class="col-12 grid-margin stretch-card">
                    <div class="card">
                        <div class="card-body">
                            @*<p class="card-description">
                                    @Model.UserName Details
                                </p>*@
                            <form class="forms-sample">
                                <div class="form-group">
                                    <label for="userName">Name</label>
                                    <label type="text" class="form-control" id="userName"> @Model.UserName</label>
                                </div>
                                <div class="form-group">
                                    <label for="userEmail">Email </label>
                                    <label type="email" class="form-control" id="userEmail">@Model.Email</label>
                                </div>
                                <div class="form-group">
                                    <label for="userPhone">Phone </label>
                                    <label type="phone" class="form-control" id="userPhone">@Model.PhoneNumber</label>
                                </div>
                                @Html.ActionLink("Back to List", "User", "", new { @class = "btn btn-primary mr-2" })
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
